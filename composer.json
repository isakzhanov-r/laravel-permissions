{
    "name": "isakzhanov-r/laravel-permissions",
    "description": "Package for using user roles and permissions in project laravel",
    "keywords": ["laravel", "permissions", "user roles", "model permission", "user permissions", "roles permissions"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/isakzhanov-r/laravel-permissions/issues",
        "source": "https://github.com/isakzhanov-r/laravel-permissions"
    },
    "authors": [
        {
            "name": "isakzhanovr",
            "email": "isakzhanov.r@gmail.com",
            "homepage": "https://github.com/isakzhanov-r/laravel-permissions"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/auth": "^11.0 || ^12.0",
        "illuminate/cache": "^11.0 || ^12.0",
        "illuminate/console": "^11.0 || ^12.0",
        "illuminate/database": "^11.0 || ^12.0",
        "illuminate/http": "^11.0 || ^12.0",
        "illuminate/support": "^11.0 || ^12.0",
        "symfony/http-kernel": "^7.0",
        "symfony/finder": "^7.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "phpunit/phpunit": "^10.0|| ^11.0 || ^12.0",
        "orchestra/testbench": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "IsakzhanovR\\Permissions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "IsakzhanovR\\Permissions\\ServiceProvider"
            ]
        }
    }
}
